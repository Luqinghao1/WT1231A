   <string notr="true">/* 全局字体与颜色：强制黑色 */
QWidget {
    color: #000000;
    font-family: &quot;Microsoft YaHei&quot;, &quot;Segoe UI&quot;, sans-serif;
    font-size: 9pt;
}

/* 工具栏背景 */
QFrame#frameToolbar {
    background-color: #f9f9f9;
    border-bottom: 1px solid #dcdcdc;
}

/* 现代化按钮样式 */
QPushButton {
    background-color: #ffffff;
    border: 1px solid #c0c0c0;
    border-radius: 4px;
    padding: 5px 15px;
    color: #333333;
    font-weight: normal;
}

QPushButton:hover {
    background-color: #f0f0f0;
    border-color: #a0a0a0;
    color: #000000;
}

QPushButton:pressed {
    background-color: #e0e0e0;
    border-color: #808080;
    padding-top: 6px;
    padding-left: 16px;
}

QPushButton:disabled {
    background-color: #f5f5f5;
    color: #a0a0a0;
    border-color: #e0e0e0;
}

/* Tab页签样式优化 */
QTabWidget::pane {
    border: 1px solid #d0d0d0;
    background: #ffffff;
    top: -1px;
}

QTabBar::tab {
    background: #e8e8e8;
    border: 1px solid #d0d0d0;
    padding: 6px 15px;
    margin-right: 2px;
    border-top-left-radius: 4px;
    border-top-right-radius: 4px;
    color: #555555;
    min-width: 80px;
}

QTabBar::tab:selected, QTabBar::tab:hover {
    background: #ffffff;
    color: #000000;
}

QTabBar::tab:selected {
    border-bottom-color: #ffffff;
    font-weight: bold;
}
</string>
